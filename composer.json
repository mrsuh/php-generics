{
    "name": "php-generics/library",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {"Mrsuh\\PhpGenerics\\": "src/"}
    },
    "extra": {
        "class": "Mrsuh\\PhpGenerics\\Command\\DumpCommand"
    },
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^1.0|^2.0",
        "php-generics/parser": "^1.1",
        "symfony/console": "^5.3",
        "symfony/filesystem": "^5.3",
        "symfony/finder": "^5.3"
    },
    "require-dev": {
        "composer/composer": "^1.0.2|^2.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@gitlab.com:php-generics/parser.git"
        }
    ]
}
